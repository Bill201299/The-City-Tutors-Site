{% extends "base/base.html" %}
{% load i18n %}
{% load crispy_forms_tags %}
{% block content %}
<div class="row gy-2 gx-2">
    <div class="col-sm">
            <h1>Get Help</h1>
            <div class="card my-1">
              <div class="card-body">
                <h5 class="card-title">K-12 Tutoring</h5>
                <p class="card-text">For parents seeking tutoring for their children in grades K-12</p>
                <a href="#" class="btn btn-primary">Register</a>
              </div>
            </div>
            <div class="card my-1">
              <div class="card-body">
                <h5 class="card-title">College Tutoring</h5>
                <p class="card-text">For students seeking tutoring in college level courses</p>
                <a href="#" class="btn btn-primary">Register</a>
              </div>
            </div>
            <div class="card my-1">
              <div class="card-body">
                <h5 class="card-title">Adult Tutoring</h5>
                <p class="card-text">For adults seeking tutoring in High School Equivalency</p>
                <a href="#" class="btn btn-primary">Register</a>
              </div>
            </div>
    </div>
    <div class="col-sm">
            <h1>Give Help</h1>
            <div class="card py-1">
              <div class="card-body">
                <h5 class="card-title">Volunteer</h5>
                <p class="card-text">Volunteer to tutor anything you want</p>
                <a href="#" class="btn btn-primary">Register</a>
              </div>
            </div>
    </div>
</div>
{% endblock %}
